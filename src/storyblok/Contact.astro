---
import { storyblokEditable } from "@storyblok/astro";
import StoryblokComponent from "@storyblok/astro/StoryblokComponent.astro";
const { blok } = Astro.props;
---

<section>
	<div class="relative min-h-screen bg-slate-900">
		<section class="flex flex-col items-center justify-center pt-24" id="contact" {...storyblokEditable(blok)}>
			<h3 class="pb-8 text-center text-4xl font-bold text-white">{blok.title}</h3>
			{
				blok.content?.map((blok) => {
					return <StoryblokComponent blok={blok} />;
				})
			}
		</section>
	</div>
</section>
